pkgname=pxz
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'aarch64')
url="https://github.com/jnovy/pxz.git"
license=('GPL')
source=("https://github.com/jnovy/pxz.git")
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir
	make
}

package()
{
	cd_src_pkg_dir
	make install DESTDIR=$pkgdir
}
