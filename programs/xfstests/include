need_kconfig:
% if job['fs'] == "f2fs"
- F2FS_FS: m
- F2FS_FS_XATTR: y
- F2FS_FS_SECURITY: y
- F2FS_FS_ENCRYPTION: y
% elsif job['fs'] == "ext2"
- EXT2_FS: m
- EXT2_FS_XATTR: y
- EXT2_FS_POSIX_ACL: y
- EXT2_FS_SECURITY: y
% elsif job['fs'] == "ext4"
- EXT4_ENCRYPTION: y
% elsif job['fs'] == "xfs"
- XFS_RT: y
- XFS_ONLINE_SCRUB: y
- XFS_ONLINE_REPAIR: y
% elsif job['fs'] == "udf"
- UDF_FS: m
% elsif job['fs'] == "ocfs2"
- OCFS2_FS: m
- OCFS2_FS_O2CB: m
- OCFS2_FS_USERSPACE_CLUSTER: m
- OCFS2_FS_STATS: y
% elsif job['fs'] == "btrfs"
- DM_DUST
% elsif job['fs'] == "bcachefs"
- BCACHEFS_FS: m
% end

- FS_ENCRYPTION: y
- DEBUG_KERNEL: y
- FAULT_INJECTION: y
- FAIL_MAKE_REQUEST: y
- FAULT_INJECTION_DEBUG_FS: y
- DM_LOG_WRITES: m
- UNICODE: m
- IO_URING: y
- EXPERT: y
