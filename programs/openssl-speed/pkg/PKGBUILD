pkgname=openssl-speed
pkgver=git
pkgrel=1
url='http://www.openssl.org/'
arch=('i386' 'x86_64' 'riscv64' 'aarch64')
license=('Apache License 2.0')
source=("$pkgname"::"https://github.com/openssl/openssl.git")
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	./config no-zlib
	make -j $(nproc)
}

package()
{
	pack_src_pkg_contents
}
