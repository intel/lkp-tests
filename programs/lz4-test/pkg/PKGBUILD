pkgname=lz4-test
pkgver=git
pkgrel=1
url='https://github.com/lz4/lz4/'
arch=('i386' 'x86_64' 'riscv64' 'aarch64')
license=('GPL2' 'BSD')
source=("$pkgname"::"https://github.com/lz4/lz4.git")
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	make -j $(nproc)
}

package()
{
	pack_src_pkg_contents lz4
}
