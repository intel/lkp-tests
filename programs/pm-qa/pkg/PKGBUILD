pkgname=pm-qa
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'aarch64')
url="https://git.linaro.org/power/pm-qa.git"
license=('GPL')
source=('https://git.linaro.org/power/pm-qa.git')
sha256sums=('SKIP')

. $LKP_SRC/lib/env.sh
. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	make_src_pkg -- -C cpuidle cpuidle_killer

	cd_src_pkg_dir $pkgname/utils
	make
}

package()
{
	pack_src_pkg_contents
}
