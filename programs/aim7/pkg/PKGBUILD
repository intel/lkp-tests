pkgname=aim7
pkgver=1
pkgrel=1
url='http://nchc.dl.sourceforge.net/project/aimbench/aim-suite7'
arch=('i386' 'x86_64' 'aarch64')
license=('GPL')
source=('http://nchc.dl.sourceforge.net/project/aimbench/aim-suite7/Initial%20release/s7110.tar.Z')
md5sums=('2b61bbc62ebc6e93af50f190c3ece119')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	make <<-END


1
gcc
-O2


n
END
	make
}

package()
{
	pack_src_pkg_execs
	pack_src_pkg_contents fakeh.tar

	DESTDIR=$benchmark_path/workfiles pack_src_pkg_contents "workfile.*"
}
