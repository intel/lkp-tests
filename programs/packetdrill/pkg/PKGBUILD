pkgname=packetdrill
pkgver=git
pkgrel=1
url='https://github.com/google/packetdrill'
arch=('x86_64' 'aarch64')
license=('GPL')
source=('https://github.com/google/packetdrill.git')
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir ${pkgname}/gtests/net/packetdrill

	./configure
	CPPFLAGS="" make
}

package()
{
	pack_src_pkg_contents
}
