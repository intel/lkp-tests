pkgname=turbostat
pkgver=git
pkgrel=1
url='https://git.kernel.org/pub/scm/linux/kernel/git/torvalds/linux.git'
arch=('i386' 'x86_64' 'aarch64')
license=('GPL')
source=('https://git.kernel.org/pub/scm/linux/kernel/git/torvalds/linux.git')
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir linux/tools/power/x86/turbostat
	make
}

package()
{
	pack_contents $srcdir/linux/tools/power/x86/turbostat/turbostat $benchmark_path
}
