pkgname=bpftrace
pkgver=git
pkgrel=1
pkgdesc="bpftrace is a high-level tracing language for Linux enhanced Berkeley Packet Filter (eBPF) available in recent Linux kernels"
arch=('x86_64' 'aarch64')
url="https://github.com/iovisor/bpftrace"
license=(Apache License V2)
depends=('')
source=('https://github.com/iovisor/bpftrace.git')
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

package()
{
    mkdir -p $benchmark_path

    wget -O $benchmark_path/bpftrace https://github.com/iovisor/bpftrace/releases/download/v0.17.0/bpftrace
    chmod +x $benchmark_path/bpftrace
}
