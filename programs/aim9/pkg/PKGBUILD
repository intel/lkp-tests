pkgname=aim9
pkgver=1
pkgrel=1
url='http://nchc.dl.sourceforge.net/project/aimbench/aim-suite9'
arch=('i386' 'x86_64' 'aarch64')
license=('GPL')
source=('http://gentwo.org/benchmark/s9110.tar.Z')
md5sums=('187ee75a79e0403214e12c3dc215f759')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	make <<-END


gcc
-O


END
	make
}

package()
{
	pack_src_pkg_execs
	pack_src_pkg_contents fakeh.tar

	cd_src_pkg_dir
	cp -a s9workfile $benchmark_path/s9workfile.all
}
