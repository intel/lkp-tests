pkgname=kvm-unit-tests
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'aarch64')
url="https://gitlab.com/kvm-unit-tests/kvm-unit-tests"
license=('GPL')
source=("https://gitlab.com/kvm-unit-tests/kvm-unit-tests.git")
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	./configure
	unset LDFLAGS
	make
}

package()
{
	pack_src_pkg_contents
}
