pkgname=redis
pkgver=7.0.9
pkgrel=0
arch=('i386' 'x86_64' 'aarch64')
url="https://redis.io"
license=('BSD')
depends=()
source=("http://download.redis.io/releases/redis-$pkgver.tar.gz")
sha256sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir $pkgname-$pkgver
	make
}

package()
{
	cd_src_pkg_dir $pkgname-$pkgver

	make install PREFIX=$benchmark_path
}
