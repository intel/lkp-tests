pkgname=redis
pkgver=git
pkgrel=0
arch=('i386' 'x86_64' 'aarch64')
url="https://redis.io"
license=('BSD')
depends=()
source=("https://github.com/redis/redis.git#branch=unstable")
sha256sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	make
}

package()
{
	cd_src_pkg_dir

	make install PREFIX=$benchmark_path
}
