pkgname=memtier
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'aarch64')
url="https://github.com/RedisLabs/memtier_benchmark"
license=('GPL2 ')
depends=()
source=("$pkgname"::"https://github.com/RedisLabs/memtier_benchmark.git")
sha256sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	autoreconf -ivf
	./configure
	make
}

package()
{
	pack_src_pkg_contents memtier_benchmark
}
