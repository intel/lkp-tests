pkgname=trace-cmd
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'aarch64')
url="https://git.kernel.org/pub/scm/linux/kernel/git/rostedt/trace-cmd.git"
license=('GPL')
source=("https://git.kernel.org/pub/scm/linux/kernel/git/rostedt/trace-cmd.git")
md5sums=('SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

prepare()
{
	prepare_benchmark_path
}

build()
{
	cd_src_pkg_dir

	make
}

package()
{
	cd_src_pkg_dir

	make install prefix=$benchmark_path
}
