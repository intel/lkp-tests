pkgname=fxmark
pkgver=git
pkgrel=1
url='https://github.com/sslab-gatech/fxmark'
arch=('i386' 'x86_64' 'aarch64')
license=('GPL')
source=('https://github.com/rli9/fxmark.git' 'https://github.com/filebench/filebench.git')
md5sums=('SKIP' 'SKIP')

. $LKP_SRC/lib/tests/pkgbuild.sh

build()
{
	make_src_pkg

	cd_src_pkg_dir filebench
	git checkout 1.4.9.1

	pkgname=filebench make_src_pkg --prefix ${benchmark_path}/filebench
}

package()
{
	pkgname=filebench make_install_src_pkg

	pack_src_pkg_contents
}
